    "@vue/test-utils": "^2.4.0",
    "typescript": "^5.3.0",
    "vite": "^6.4.1",
    "vitest": "^4.0.17",
    "vue-tsc": "^2.0.0"
  },
  "keywords": [
    "language-models",
    "education",
    "inference",
    "vue",
    "typescript"
